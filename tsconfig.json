{
  "display": "Node LTS (22)",
  "_version": "22.0.0",
  "compilerOptions": {
    "lib": [
      "es2023"
    ],
    "module": "node16",
    "target": "es2022",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "moduleResolution": "node16",
    "outDir": "build",
    "noUncheckedIndexedAccess": true,
    "noEmit": true,
  },
  "include": [
    "src/**/*.ts"
  ],
  "exclude": [
    "src/web"
  ]
}